{
  "name": "bip39-ng-app",
  "version": "6.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve --open",
    "eject": "ng eject",
    "http-server": "http-server dist/ -c-1",
    "build-all": "npm run-script build && npm run-script build-dev && npm run-script build-gh",
    "build": "ng build --prod --output-path ./dist/",
    "build-dev": "ng build --output-path ./dist-dev/",
    "build-gh": "ng build --aot=false --base-href /bip39-ng/ --deploy-url /bip39-ng/ --output-path ./docs/ ",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "update": "ng update",
    "ci": "npm run lint && npm run test && npm run e2e",
    "sme": "ng build && source-map-explorer dist/main.bundle.js",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && git push --follow-tags origin master",
    "docker": "docker build -t bip39-ng-app . && docker run -d -p 4200:80 bip39-ng-app",
    "postinstall": "node patch.js"
  },
  "private": true,
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.0.0"
  },
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/flex-layout": "5.0.0-beta.14",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "@types/bitcoinjs-lib": "^3.4.0",
    "bip32-utils": "^0.11.1",
    "bip39": "^2.5.0",
    "bitcoinjs-lib": "^3.3.2",
    "blockchain.info": "^2.12.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.6",
    "rxjs": "6.1.0",
    "rxjs-compat": "6.1.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.0",
    "@angular-devkit/build-ng-packagr": "~0.6.0",
    "@angular/cdk": "6.0.1",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@angular/material": "6.0.1",
    "@angular/platform-server": "6.0.0",
    "@angular/service-worker": "6.0.0",
    "@ngx-translate/core": "10.0.1",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/html2canvas": "0.0.33",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.0.4",
    "angular-cli-ghpages": "0.5.2",
    "codelyzer": "4.3.0",
    "coveralls": "3.0.1",
    "hammerjs": "2.0.8",
    "html2canvas": "1.0.0-alpha.12",
    "http-server": "0.11.1",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.0.0",
    "karma-remap-istanbul": "0.6.0",
    "karma-scss-preprocessor": "3.0.0",
    "moment": "2.22.1",
    "ng-packagr": "^3.0.0-rc.2",
    "node-sass": "4.9.0",
    "protractor": "5.3.1",
    "source-map-explorer": "1.5.0",
    "standard-version": "4.3.0",
    "ts-helpers": "1.1.2",
    "ts-node": "6.0.3",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "5.10.0",
    "typescript": "2.7.2"
  }
}
