{
  "name": "bip39-ng-app",
  "version": "6.0.0",
  "license": "MIT",
  "scripts": {
    "electron": "npm run-script build-electron && electron electron-app.js",
    "start": "ng serve --open",
    "eject": "ng eject",
    "http-server": "http-server dist/ -c-1",
    "build-all": "npm run-script build && npm run-script build-dev && npm run-script build-gh",
    "build": "ng build --configuration production --aot=true --output-path ./dist/",
    "build-dev": "ng build --aot=true --output-path ./dist-dev/",
    "build-electron": "ng build --aot=false --base-href ./ --output-path ./dist-electron/",
    "build-gh": "ng build --configuration production --aot=false --base-href /bip39-ng/ --output-path ./docs/ ",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "update": "ng update",
    "ci": "npm run lint && npm run test && npm run e2e",
    "sme": "ng build && source-map-explorer dist/main.bundle.js",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && git push --follow-tags origin master",
    "docker": "docker build -t bip39-ng-app . && docker run -d -p 4200:80 bip39-ng-app",
    "postinstall": "node patch.js"
  },
  "private": true,
  "engines": {
    "node": ">= 13.7.0",
    "npm": ">= 6.13.0"
  },
  "dependencies": {
    "@angular/animations": "12.2.0",
    "@angular/common": "12.2.0",
    "@angular/compiler": "12.2.0",
    "@angular/core": "12.2.0",
    "@angular/flex-layout": "12.0.0-beta.34",
    "@angular/forms": "12.2.0",
    "@angular/material": "12.2.0",
    "@angular/platform-browser": "12.2.0",
    "@angular/platform-browser-dynamic": "12.2.0",
    "@angular/router": "12.2.0",
    "big-integer": "^1.6.48",
    "bip32": "^2.0.6",
    "bip39": "^3.0.4",
    "bip84": "^0.2.5",
    "bitcoinjs-lib": "^5.2.0",
    "classlist.js": "1.1.20150312",
    "core-js": "3.16.1",
    "material-design-icons": "^3.0.1",
    "ngx-moment": "^5.0.0",
    "randombytes": "^2.1.0",
    "rxjs": "7.3.0",
    "rxjs-compat": "6.6.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cdk": "^12.2.0",
    "@angular/cli": "^12.2.0",
    "@angular/compiler-cli": "^12.2.0",
    "@angular/language-service": "12.2.0",
    "@angular/platform-server": "12.2.0",
    "@angular/service-worker": "^12.2.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/html2canvas": "0.0.36",
    "@types/jasmine": "3.8.2",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "^16.4.13",
    "angular-cli-ghpages": "0.6.2",
    "codelyzer": "^6.0.2",
    "coveralls": "3.1.1",
    "electron": "^13.1.9",
    "hammerjs": "2.0.8",
    "html2canvas": "1.2.2",
    "http-server": "^13.0.0",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "karma-remap-istanbul": "0.6.0",
    "karma-scss-preprocessor": "4.0.0",
    "moment": "^2.29.1",
    "ng-packagr": "^12.2.0",
    "protractor": "^7.0.0",
    "sass": "^1.37.5",
    "source-map-explorer": "2.5.2",
    "standard-version": "9.3.1",
    "ts-helpers": "1.1.2",
    "ts-node": "10.2.0",
    "tslint": "5.20.1",
    "typescript": "4.3.5"
  }
}
