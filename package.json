{
  "name": "bip39-ng-app",
  "version": "6.0.0",
  "license": "MIT",
  "scripts": {
    "electron": "npm run-script build-electron && electron electron-app.js",
    "start": "ng serve --open",
    "eject": "ng eject",
    "http-server": "http-server dist/ -c-1",
    "build-all": "npm run-script build && npm run-script build-dev && npm run-script build-gh",
    "build": "ng build --prod --aot=true --output-path ./dist/",
    "build-dev": "ng build --aot=true --output-path ./dist-dev/",
    "build-electron": "ng build --aot=false --base-href ./ --output-path ./dist-electron/",
    "build-gh": "ng build --prod --aot=false --base-href /bip39-ng/ --output-path ./docs/ ",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "update": "ng update",
    "ci": "npm run lint && npm run test && npm run e2e",
    "sme": "ng build && source-map-explorer dist/main.bundle.js",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && git push --follow-tags origin master",
    "docker": "docker build -t bip39-ng-app . && docker run -d -p 4200:80 bip39-ng-app",
    "postinstall": "node patch.js"
  },
  "private": true,
  "engines": {
    "node": ">= 13.7.0",
    "npm": ">= 6.13.0"
  },
  "dependencies": {
    "@angular/animations": "9.1.11",
    "@angular/common": "9.1.11",
    "@angular/compiler": "9.1.11",
    "@angular/core": "9.1.11",
    "@angular/flex-layout": "9.0.0-beta.31",
    "@angular/forms": "9.1.11",
    "@angular/material": "9.2.4",
    "@angular/platform-browser": "9.1.11",
    "@angular/platform-browser-dynamic": "9.1.11",
    "@angular/router": "9.1.11",
    "big-integer": "^1.6.48",
    "bip32": "^2.0.5",
    "bip39": "^3.0.2",
    "bitcoinjs-lib": "^5.1.7",
    "classlist.js": "1.1.20150312",
    "core-js": "3.6.1",
    "material-design-icons": "^3.0.1",
    "ngx-moment": "^3.5.0",
    "rxjs": "6.5.4",
    "rxjs-compat": "6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.9",
    "@angular-devkit/build-ng-packagr": "~0.901.9",
    "@angular/cdk": "^9.1.11",
    "@angular/cli": "^9.1.9",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "9.1.11",
    "@angular/platform-server": "9.1.11",
    "@angular/service-worker": "^9.1.11",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/html2canvas": "0.0.35",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "0.6.1",
    "codelyzer": "^5.1.2",
    "coveralls": "3.0.9",
    "electron": "^7.1.10",
    "hammerjs": "2.0.8",
    "html2canvas": "1.0.0-rc.5",
    "http-server": "^0.12.3",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.5.1",
    "karma-remap-istanbul": "0.6.0",
    "karma-scss-preprocessor": "4.0.0",
    "moment": "^2.24.0",
    "ng-packagr": "^9.0.0",
    "node-sass": "^4.14.1",
    "protractor": "^7.0.0",
    "source-map-explorer": "2.2.0",
    "standard-version": "7.0.1",
    "ts-helpers": "1.1.2",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "typescript": "3.8.3"
  }
}
