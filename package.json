{
  "name": "bip39-ng-app",
  "version": "6.0.0",
  "license": "MIT",
  "scripts": {
    "electron": "npm run-script build-electron && electron electron-app.js",
    "start": "ng serve --open",
    "eject": "ng eject",
    "http-server": "http-server dist/ -c-1",
    "build-all": "npm run-script build && npm run-script build-dev && npm run-script build-gh",
    "build": "ng build --prod --aot=true --output-path ./dist/",
    "build-dev": "ng build --aot=true --output-path ./dist-dev/",
    "build-electron": "ng build --aot=false --base-href ./ --output-path ./dist-electron/",
    "build-gh": "ng build --prod --aot=false --base-href /bip39-ng/ --output-path ./docs/ ",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "update": "ng update",
    "ci": "npm run lint && npm run test && npm run e2e",
    "sme": "ng build && source-map-explorer dist/main.bundle.js",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && git push --follow-tags origin master",
    "docker": "docker build -t bip39-ng-app . && docker run -d -p 4200:80 bip39-ng-app",
    "postinstall": "node patch.js"
  },
  "private": true,
  "engines": {
    "node": ">= 13.7.0",
    "npm": ">= 6.13.0"
  },
  "dependencies": {
    "@angular/animations": "11.2.5",
    "@angular/common": "11.2.5",
    "@angular/compiler": "11.2.5",
    "@angular/core": "11.2.5",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.2.5",
    "@angular/material": "11.2.4",
    "@angular/platform-browser": "11.2.5",
    "@angular/platform-browser-dynamic": "11.2.5",
    "@angular/router": "11.2.5",
    "big-integer": "^1.6.48",
    "bip32": "^2.0.6",
    "bip39": "^3.0.3",
    "bip84": "^0.2.5",
    "bitcoinjs-lib": "^5.2.0",
    "classlist.js": "1.1.20150312",
    "core-js": "3.9.1",
    "material-design-icons": "^3.0.1",
    "ngx-moment": "^5.0.0",
    "rxjs": "6.6.6",
    "rxjs-compat": "6.6.6",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.4",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cdk": "^11.2.4",
    "@angular/cli": "^11.2.4",
    "@angular/compiler-cli": "^11.2.5",
    "@angular/language-service": "11.2.5",
    "@angular/platform-server": "11.2.5",
    "@angular/service-worker": "^11.2.5",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/html2canvas": "0.0.36",
    "@types/jasmine": "3.6.6",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^14.14.34",
    "angular-cli-ghpages": "0.6.2",
    "codelyzer": "^6.0.1",
    "coveralls": "3.1.0",
    "electron": "^12.0.1",
    "hammerjs": "2.0.8",
    "html2canvas": "1.0.0-rc.7",
    "http-server": "^0.12.3",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "^6.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-remap-istanbul": "0.6.0",
    "karma-scss-preprocessor": "4.0.0",
    "moment": "^2.29.1",
    "ng-packagr": "^11.2.4",
    "protractor": "^7.0.0",
    "sass": "^1.32.8",
    "source-map-explorer": "2.5.2",
    "standard-version": "9.1.1",
    "ts-helpers": "1.1.2",
    "ts-node": "9.1.1",
    "tslint": "5.20.1",
    "typescript": "4.1.3"
  }
}
