{
  "name": "bip39-ng-app",
  "version": "6.0.0",
  "license": "MIT",
  "scripts": {
    "electron": "npm run-script build-electron && electron electron-app.js",
    "start": "ng serve --open",
    "eject": "ng eject",
    "http-server": "http-server dist/ -c-1",
    "build-all": "npm run-script build && npm run-script build-dev && npm run-script build-gh",
    "build": "ng build --prod --aot=true --output-path ./dist/",
    "build-dev": "ng build --aot=true --output-path ./dist-dev/",
    "build-electron": "ng build --aot=false --base-href ./ --output-path ./dist-electron/",
    "build-gh": "ng build --prod --aot=false --base-href /bip39-ng/ --output-path ./docs/ ",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "update": "ng update",
    "ci": "npm run lint && npm run test && npm run e2e",
    "sme": "ng build && source-map-explorer dist/main.bundle.js",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && git push --follow-tags origin master",
    "docker": "docker build -t bip39-ng-app . && docker run -d -p 4200:80 bip39-ng-app",
    "postinstall": "node patch.js"
  },
  "private": true,
  "engines": {
    "node": ">= 13.7.0",
    "npm": ">= 6.13.0"
  },
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "8.2.14",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/pwa": "^0.803.25",
    "@angular/router": "8.2.14",
    "bip32": "^2.0.5",
    "bip39": "^3.0.2",
    "bitcoinjs-lib": "^5.1.7",
    "blockchain.info": "^2.12.1",
    "classlist.js": "1.1.20150312",
    "core-js": "3.6.1",
    "material-design-icons": "^3.0.1",
    "ngx-moment": "^3.5.0",
    "rxjs": "6.5.4",
    "rxjs-compat": "6.5.4",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular-devkit/build-ng-packagr": "^0.803.23",
    "@angular/cdk": "8.2.3",
    "@angular/cli": "^8.3.23",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@angular/material": "8.2.3",
    "@angular/platform-server": "8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/html2canvas": "0.0.35",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "13.1.2",
    "angular-cli-ghpages": "0.6.1",
    "codelyzer": "5.2.1",
    "coveralls": "3.0.9",
    "electron": "^7.1.10",
    "hammerjs": "2.0.8",
    "html2canvas": "1.0.0-rc.5",
    "http-server": "0.12.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.5.1",
    "karma-remap-istanbul": "0.6.0",
    "karma-scss-preprocessor": "4.0.0",
    "moment": "^2.24.0",
    "ng-packagr": "^5.7.1",
    "node-sass": "^4.13.1",
    "protractor": "5.4.2",
    "source-map-explorer": "2.2.0",
    "standard-version": "7.0.1",
    "ts-helpers": "1.1.2",
    "ts-node": "8.5.4",
    "tsickle": "^0.38.0",
    "tslib": "^1.10.0",
    "tslint": "5.20.1",
    "typescript": "3.4.5"
  }
}
